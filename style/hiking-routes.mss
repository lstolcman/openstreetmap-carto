
#black_bar {
  [zoom >= 14] {
    line/line-color: #444444;
    line/line-width: 2.5;
    line/line-dasharray: 20,5;
  }
}

#blue_bar {
  [zoom >= 14] {
    line/line-color: #0766c7;
    line/line-width: 2.5;
    line/line-dasharray: 20,5;
  }
}

#green_bar {
  [zoom >= 14] {
    line/line-color: #409716;
    line/line-width: 2.5;
    line/line-dasharray: 20,5;
  }
}

#red_bar {
  [zoom >= 14] {
    line/line-color: #e64525;
    line/line-width: 2.5;
    line/line-dasharray: 20,5;
  }
}

#yellow_bar {
  [zoom >= 14] {
    
    line/line-color: #ffe034;
    line/line-width: 2.5;
    line/line-dasharray: 20,5;
  }
}


#blue_red_bar {
  [zoom >= 14] {
    line/line-color: #0766c7;
    line/line-width: 2.5;
    line/line-dasharray: 20,5;
    line/line-offset: -2;
    line2/line-color: #e64525;
    line2/line-width: 2.5;
    line2/line-dasharray: 20,5;
    line2/line-offset: 2;
  }
}
